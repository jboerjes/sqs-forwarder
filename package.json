{
  "name": "sqs-forwarder",
  "version": "0.0.1",
  "description": "Library that lets you send messages from SQS to a REST endpoint",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist --source-maps",
    "lint": "eslint src test",
    "test": "npm run build && npm run lint && mocha test --recursive",
    "prepublish": "npm run build && npm run test",
    "prepush": "npm test",
    "start-example": "DEBUG=sqs-forwarder* && npm run build && node examples/usage.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vokrik/sqs-to-http.git"
  },
  "keywords": [
    "SQS",
    "REST",
    "converter"
  ],
  "author": "Martin Vokrinek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vokrik/sqs-to-http/issues"
  },
  "homepage": "https://github.com/vokrik/sqs-to-http#readme",
  "dependencies": {
    "aws-sdk": "^2.94.0",
    "axios": "^0.16.2",
    "debug": "^2.6.8",
    "deepmerge": "^1.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "proxyrequire": "^1.0.10",
    "sinon": "^2.4.1"
  }
}
